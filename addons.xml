<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.Gorillawizard" name="[COLOR yellow][B]Playonmonkeys Build Wizard[/B][/COLOR]" version="3.3.4" provider-name="Playonmonkey/FTG">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Playonmonkeys Build Wizard</summary>
    <description lang="en">In this wizard you will find the latest Playonmonkeys build....Without work from the Devs and Streaming Websites this would not of been possible! Thanks whufclee@ TotalXBMC,GUYS @ TI, FTG, MuckyDuck, ][NT3L][G3NC][ for the app installer and all Developers who created add-ons used in my build. Also a very special Thanks to Surfacingx for the code used in this add-on. Visit www.mediacenterwizards.com for more info and support.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.playonmonkey" name="Playonmonkey" version="1.3" provider-name="Playonmonkey">
	<extension point="xbmc.addon.repository" name="Playonmonkey">
		<info compressed="false">https://raw.githubusercontent.com/playonmonkey/Gorilla/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/playonmonkey/Gorilla/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/playonmonkey/Gorilla/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Playonmonkeys Repository</summary>
		<description>Stored add ons by Playonmonkey</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
